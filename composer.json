{
	"name": "wikimedia/common-passwords",
	"description": "List of the 100,000 most commonly used passwords",
	"license": "MIT",
	"homepage": "https://www.mediawiki.org/wiki/CommonPasswords",
	"authors": [
		{
			"name": "Sam Reed",
			"email": "reedy@wikimedia.org"
		}
	],
	"autoload": {
		"psr-4": {
			"Wikimedia\\CommonPasswords\\": "src/"
		}
	},
	"require": {
		"php": ">=7.4.3"
	},
	"require-dev": {
		"mediawiki/mediawiki-codesniffer": "41.0.0",
		"mediawiki/mediawiki-phan-config": "0.14.0",
		"mediawiki/minus-x": "1.1.1",
		"ockcyp/covers-validator": "1.6.0",
		"php-parallel-lint/php-console-highlighter": "1.0.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phpunit/phpunit": "9.6.16"
	},
	"scripts": {
		"test": [
			"parallel-lint . --exclude vendor",
			"phpunit",
			"covers-validator",
			"@phpcs",
			"phan --allow-polyfill-parser --long-progress-bar",
			"minus-x check ."
		],
		"cover": "phpunit --coverage-html coverage",
		"fix": [
			"minus-x fix .",
			"phpcbf"
		],
		"phan": "phan -d . --long-progress-bar",
		"phpcs": "phpcs -sp"
	}
}
